{
   "name": "stations",
   "$schema": "../../../node_modules/nx/schemas/project-schema.json",
   "sourceRoot": "libs/features/stations/src",
   "prefix": "lib",
   "tags": [],
   "projectType": "library",
   "targets": {
      "test": {
         "executor": "@nx/jest:jest",
         "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
         "options": {
            "jestConfig": "libs/features/stations/jest.config.ts",
            "passWithNoTests": true
         },
         "configurations": {
            "ci": {
               "ci": true,
               "codeCoverage": true
            }
         }
      },
      "lint": {
         "executor": "@nx/linter:eslint",
         "outputs": ["{options.outputFile}"],
         "options": {
            "lintFilePatterns": [
               "libs/features/stations/**/*.ts",
               "libs/features/stations/**/*.html"
            ]
         }
      }
   },
   "assets": [
      "../assets/main.scss",
      "../assets/images",
      "../assets/icons"
   ]
}
