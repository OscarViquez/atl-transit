{
  "name": "station-ui",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/stations/ui/src",
  "prefix": "station-ui",
  "tags": [],
  "projectType": "library",
  "targets": {
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/stations/ui/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "libs/stations/ui/**/*.ts",
          "libs/stations/ui/**/*.html"
        ]
      }
    }
  },
  "assets": [
    "../assets/src/lib/assets/main.scss",
    "../assets/src/lib/assets/images",
    "../assets/src/lib/assets/icons",
    "../assets/src/lib/assets/data"
  ]
}
