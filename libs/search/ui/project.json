{
   "name": "search-ui",
   "$schema": "../../../node_modules/nx/schemas/project-schema.json",
   "sourceRoot": "libs/search/ui/src",
   "prefix": "lib",
   "tags": [],
   "projectType": "library",
   "targets": {
      "test": {
         "executor": "@nx/jest:jest",
         "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
         "options": {
            "jestConfig": "libs/search/ui/jest.config.ts",
            "passWithNoTests": true
         },
         "configurations": {
            "ci": {
               "ci": true,
               "codeCoverage": true
            }
         }
      },
      "lint": {
         "executor": "@nx/linter:eslint",
         "outputs": ["{options.outputFile}"],
         "options": {
            "lintFilePatterns": [
               "libs/search/ui/**/*.ts",
               "libs/search/ui/**/*.html"
            ]
         }
      }
   }
}
